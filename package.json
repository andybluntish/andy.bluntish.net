  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://andybluntish.com",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "eslint": "^7.4.0",
    "html-minifier": "^4.0.0",
    "markdown-it": "^11.0.0",
    "prettier": "^2.0.5"
  }
}
